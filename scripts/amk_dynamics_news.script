function show_news()
 local name,sname = names()
 local reason,dead,when,where,text,voennie_news,naim_news,bandos_news,freedom_news,dolg_news,stalkers_news = news() 
     if math.random(0,2)<1 then
          if math.random(0,1)<1 then
              if math.random(0,1)<1 then
              send_tip(dolg_news,"Долг", 0, 12,"dolg")
              else
              send_tip(freedom_news,"Свобода", 0, 12,"freedom")
              end
          else
              if math.random(0,1)<1 then
              send_tip(naim_news,"Наёмники", 0, 12,"killer")
              else
              send_tip(bandos_news,"Бандиты", 0, 12,"bandit")
              end
          end
     else
         if math.random(0,2)<1 then
              if math.random(0,2)<1 then
              send_tip(voennie_news,"Военные сталкеры", 0, 12,"army")
              else
              send_tip(stalkers_news,"Сталкеры", 0, 12,"all")
              end
         else
              if math.random(0,1)<1 then
              send_tip(where.." "..when.." "..text.." ",name.." "..sname, 0, 12,"all")
              else
              send_tip("%c[255,160,160,160]"..name.." "..sname..". ".."%c[default]"..dead..". "..reason,"Погиб сталкер:", 0, 12,"deth")
              end
          end
     end
end



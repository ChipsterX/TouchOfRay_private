function news_of_surge()
	local psi = psi_storm_manager and psi_storm_manager.PsiStormManager
	local surge = surge_manager and surge_manager.SurgeManager

	local enable_blowout = atmosfear_options.config:r_value("atmosfear_current_parameters","opt_enable_blowout",2,1)==1
	local enable_psi_storm = atmosfear_options.config:r_value("atmosfear_current_parameters","opt_enable_psi_storm",2,1)==1
	local not_surge_start = surge ~= nil and surge._delta ~= nil and not surge.started
	local not_psi_start = psi ~= nil and psi._delta ~= nil and not psi.started

   if enable_blowout and enable_psi_storm then	
	if not_psi_start and not_surge_start then
		 
		-- посмотрим, что наступит первым: выброс или пси-шторм. Что раньше, о том и говорим.
		if surge._delta < psi._delta then
	        new_surge_time(surge._delta, surge.last_surge_time, "выброс")
                else
	        new_surge_time(psi._delta, psi.last_psi_storm_time, "пси-шторм")
		end
	elseif not_psi_start then
	        new_surge_time(psi._delta, psi.last_psi_storm_time, "пси-шторм")
	elseif not_surge_start then
	        new_surge_time(surge._delta, surge.last_surge_time, "выброс")
	end
   elseif enable_blowout and not_surge_start then
	        new_surge_time(surge._delta, surge.last_surge_time, "выброс")
   elseif enable_psi_storm and not_psi_start then
	        new_surge_time(psi._delta, psi.last_psi_storm_time, "пси-шторм")
   end
end

function new_surge_time(delta, last_blow_time, text)
local frases = {
	{"Слыхал от учёных, что %s по их рассчётам будет %02d.%02d.%04d", ", сказали в %02d:%02d"},
	{"Знакомый эколог шепнул мне по секрету, что %s намечается %02d.%02d.%04d", ", вроде в %02d:%02d"},
	{"Заплатил неплохую сумму за инфу, так что будете должны: %s будет %02d.%02d.%04d", ", по здешнему в %02d:%02d"},
	{"Проставитесь в баре за инфу о %sе: Зоновский будильник прозвенит %02d.%02d.%04d", ", а точнее в %02d:%02d"},
	{"Для проспавших, %s будет %02d.%02d.%04d", ", и поставьте таймер на %02d:%02d"},
	{"Друг внука моей троюродной тёти в Иваново молвил, что %s будет %02d.%02d.%04d", ", берегитесь в %02d:%02d"},
	{"Предлагаю прятаться по норам от %sа %02d.%02d.%04d", ", особенно в %02d:%02d"},
	{"Кто не хочет умереть страшной смертью, остерегайтесь %sа %02d.%02d.%04d", ", и не выходите на свежий воздух в %02d:%02d"},
	{"Хоть от бабушки ушёл, хоть от дедушки, но %s всё равно будет %02d.%02d.%04d", ", лучше прятаться заранее. Будет в %02d:%02d"},
}
	local name,sname = names()
	local spammer = name.." "..sname
		local g_time = game.get_game_time()
		local blow_start = math.floor(delta - g_time:diffSec(last_blow_time))
		local Y, M, D, h, m, s, ms = 0, 0, 0, 0, 0, 0, 0
		local next_time = game.get_game_time()
		Y, M, D, h, m, s, ms = next_time:get(Y, M, D, h, m, s, ms)
		next_time:set(Y, M, D, h, m, s + blow_start, ms)
		Y, M, D, h, m, s, ms = next_time:get(Y, M, D, h, m, s, ms)
		db.actor:give_game_news("Сообщение:",(spammer .. "\\n%c[255,228,137,143]" .. string.format(frases[math.random(1, #frases)][1],text,D, M, Y) .. string.format(frases[math.random(1, #frases)][2],h,m)), "ui_ecolog_grouping_vs", 0, 5000)
		xr_sound.set_sound_play(db.actor:id(), "pda_tips")
end



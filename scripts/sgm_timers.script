function timers_main()
   timers_update() 
end
function timers_update()
   --//Таймер МП-3 плеера//-->
   if ui_mod_elements.mp3_plays~=nil then
      ui_mod_elements.mp3_length_position=string.format(math.floor(time_global()/1000))-ui_mod_elements.mp3_last_position
   end
end
